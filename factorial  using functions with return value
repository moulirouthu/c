#include <stdio.h>
int fact(int);
int main()
{
	int n,factorial;
 
  	printf("Enter a number:");
  	scanf("%d",&n);
  	factorial=fact(n);
    printf("\nFactorial of the %d = %d",n,factorial) ;
}

int fact(int n)
{
    int i,f=1;
    for(i=1;i<=n;i++)
    {
        f=f*i;
    }
    return f;
}

Output:
Enter a number:5
Factorial of the 5 = 120
